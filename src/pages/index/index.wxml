<view>
  <van-search
    placeholder='城市/拼音/首字母'
    background='#FAFAFA'
    value='{{inputVal}}'
    bind:change='onChange'>
  </van-search>
  <view class='weui-grids'>
    <block wx:for='{{cityList}}' wx:key='*thiscity'>
      <view class='weui-grid' hover-class='weui-grid_active' id='app{{item.key}}' data-key='{{item.key}}' bindtap='tapCity'>
        <image class='weui-grid__icon' src='../../images/logo/logo_{{item.key}}.png' />
        <view class='weui-grid__label'>{{item.name}}</view>
      </view>
    </block>
  </view>
</view>